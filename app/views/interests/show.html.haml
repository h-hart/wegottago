%section
  = render 'shared/top-nav'
%section.categories
  .container
    - @activity_categories.each do |category|
      .row
        =link_to activity_category_activities_explore_path(activity_category_id: category.id) do
          .col-md-4.category
            = image_tag category.small_image_url, class: ''
          .col-md-8.category
            - get_duplicates(category.interests, @user.interests ).each do |i|
              %span.interest
                %span.cval= i.name