=link_to activity_category_activities_explore_path(activity_category_id: category.id) do
  .col-md-4.category
    .category-image
      = image_tag category.small_image_url
    .category-info
      .row.category-followers
        - category.interested_users.limit(6).each do |user|
          .col-sm-2.posted_by
            %a{ href: profile_path(user.id) }
              = image_tag user.avatar.thumb.url, class: "posted_by_pic"
